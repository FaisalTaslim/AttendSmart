                                </fieldset>

                                <fieldset>
                                    <legend>Terms & Conditions</legend>

                                    <div class="form-group terms-wrapper">
                                        <input type="checkbox" class="terms-check" name="termsCheck" required>
                                        <span class="terms-text">
                                            I agree to the
                                            <a href="/terms" target="_blank">Terms & Conditions</a>
                                        </span>
