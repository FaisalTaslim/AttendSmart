<body>
    <% if (popupMessage) { %>
        <div id="popup" class="popup <%= popupType %> show">
            <%= popupMessage %>
        </div>
        <% } %>

            <header>
                <div class="logo">
                    <h1 class="pc-hidden">AttendSmart - Smart Attendance Solution</h1>
                    <h1 class="mobile-hidden">AttendSmart</h1>
                </div>
                <nav class="nav-bar nav-select">
                    <% if (isAuthenticated) { %>
                        <a href="/dashboard" class="nav-links-property">Dashboard</a>
                        <a href="/logout" class="nav-links-property">Logout</a>
                        <a href="/guidebook" class="nav-links-property">GuideBook</a>
                        <% } else { %>
                            <p class="nav-links-property login-page-btn">Login</p>
                            <a href="/guidebook" class="nav-links-property">GuideBook</a>
                            <% } %>
                </nav>
                <nav class="mobile-nav-container nav-select">
                    <div class="form-group">
                        <select id="mobile-nav">
                            <option value="">Navigate</option>
