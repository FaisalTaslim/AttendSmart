                                    </div>

                                    <div class="outer-group">
                                        <input type="text" name="orgName" placeholder="Organization Name" required>
                                        <input type="text" name="orgBranch" placeholder="Branch" required>
                                    </div>
                                </fieldset>

                                <fieldset>
                                    <legend>Subject Details</legend>

                                    <div class="outer-group">
                                        <input type="text" name="subjects" placeholder="Subject 1">
                                        <input type="text" name="subjects" placeholder="Subject 2">
                                        <input type="text" name="subjects" placeholder="Subject 3">
                                        <input type="text" name="subjects" placeholder="Subject 4">
