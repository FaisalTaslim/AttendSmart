                            <form class="form-box r-forms hidden-mob" id="e-form" enctype="multipart/form-data">
                                <fieldset>
                                    <legend>Employee Registration</legend>

                                    <div class="outer-group">
                                        <input type="text" name="userName" placeholder="Full Name" required>
                                        <input type="text" name="employeeId" placeholder="Employee ID" required>
                                        <input type="text" name="designation" placeholder="Designation / Job Role"
                                            required>
                                    </div>

                                    <div class="outer-group">
                                        <select name="workType" required>
                                            <option value="" disabled selected>Works At</option>
                                            <option value="school">School</option>
                                            <option value="college">College</option>
                                            <option value="corporate">Corporate</option>
                                        </select>

                                        <select name="shift" required>
                                            <option value="" disabled selected>Select Shift</option>
                                            <option value="day">Day</option>
                                            <option value="night">Night</option>
                                        </select>
                                    </div>

                                    <div class="outer-group">
                                        <input type="text" name="orgName" placeholder="Organization Name" required>
                                        <input type="text" name="orgBranch" placeholder="Organization Branch" required>
                                    </div>

                                    <div class="outer-group">
                                        <input type="tel" name="contact" placeholder="Contact Number" required>
                                        <input type="email" name="email" placeholder="Email Address" required>
                                    </div>
                                </fieldset>

                                <fieldset>
                                    <legend>Password Setup</legend>

                                    <div class="outer-group">
                                        <input type="password" class="password" name="password"
                                            placeholder="Set Password" required>
                                        <input type="password" class="confirm-password" name="confirmPassword"
                                            placeholder="Confirm Password" required>
                                    </div>
                                </fieldset>

                                <fieldset>
                                    <legend>Face Verification</legend>

                                    <div class="form-group">
                                        <input type="file" name="faceImage" id="faceImage3" accept="image/*">
                                        <img style="display:none;">
                                    </div>
                                </fieldset>

                                <fieldset>
                                    <legend>Terms & Conditions</legend>

                                    <div class="terms-wrapper">
                                        <input type="checkbox" class="terms-check" name="termsCheck" required>

                                        <span class="terms-text">
                                            I agree to the
                                            <a href="/terms" target="_blank">Terms & Conditions</a>
